{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
      "no-console": 0,
      "no-await-in-loop": 2,
      "valid-jsdoc": 2,
      "eqeqeq": 2,
      "curly": 2,
      "no-return-await": 2
    }
}
